{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Test CF template to demonstrate nested stack creation",
    "Parameters": {
        "Environment": {
            "Type": "String",
            "Default": "dev"
        },
        "ProjectName": {
            "Type": "String",
            "Default": "gritcoding"
        }
    },
    "Resources": {
        "S3CatStack": {
            "Type": "AWS::CloudFormation::Stack",
            "Properties": {
                "Parameters": {
                    "ProjectName": {
                        "Ref": "ProjectName"
                    },
                    "Environment": {
                        "Ref": "Environment"
                    },
                    "Application": "cat",
                    "ExpirationInDays": 0
                },
                "TemplateURL": "https://s3.us-east-1.amazonaws.com/grit-coding-template-build-demo/f5dee74f266de63a4041364193b2c898.template"
            }
        },
        "S3DogStack": {
            "Type": "AWS::CloudFormation::Stack",
            "Properties": {
                "Parameters": {
                    "ProjectName": {
                        "Ref": "ProjectName"
                    },
                    "Environment": {
                        "Ref": "Environment"
                    },
                    "Application": "dog",
                    "ExpirationInDays": 0
                },
                "TemplateURL": "https://s3.us-east-1.amazonaws.com/grit-coding-template-build-demo/f5dee74f266de63a4041364193b2c898.template"
            }
        }
    }
}